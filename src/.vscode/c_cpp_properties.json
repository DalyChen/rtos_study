{
    "env": {
        "McuInclude": "${workspaceFolder}/MCU",
        "SystemInclude": "D:/temp_keil_include/include"
    },
    "configurations": [
        {
            "name": "windows",
            "defines": ["STM32F40_41xxx"],
            "includePath": [
                "${SystemInclude}",
                "${workspaceFolder}/app",
                "${workspaceFolder}/bsp",
                "${workspaceFolder}/ucos_ii",
                "${workspaceFolder}/ucos_ii/Ports/ARM-Cortex-M4/RealView",
                "${workspaceFolder}/ucos_ii_cfg",
                "${workspaceFolder}/ucos_ii_cpu",
                "${workspaceFolder}/ucos_ii_cpu/ARM-Cortex-M4/RealView",
                "${workspaceFolder}/ucos_ii_lib",
                "${McuInclude}/CMSIS_M4",
                "${McuInclude}/STM32F4XX",
                "${McuInclude}/STM32F4xx_DSP_StdPeriph/inc"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${SystemInclude}",
                    "${workspaceFolder}/app",
                    "${workspaceFolder}/bsp",
                    "${workspaceFolder}/ucos_ii",
                    "${workspaceFolder}/ucos_ii_cfg",
                    "${workspaceFolder}/ucos_ii_cpu",
                    "${workspaceFolder}/ucos_ii_cpu/ARM-Cortex-M4/RealView",
                    "${workspaceFolder}/ucos_ii_lib",
                    "${McuInclude}/CMSIS_M4",
                    "${McuInclude}/STM32F4XX",
                    "${McuInclude}/STM32F4xx_DSP_StdPeriph/inc"
                ]
            },
            "cStandard": "c99",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}